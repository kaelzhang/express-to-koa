{
  "name": "express-to-koa",
  "version": "1.0.6",
  "description": "Use express middlewares in Koa2, the one that really works.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "npm run build && node --harmony ./node_modules/.bin/ava --verbose --timeout=10s",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/express-to-koa.git"
  },
  "keywords": [
    "express-to-koa",
    "express",
    "koa",
    "koa2",
    "middleware",
    "converter",
    "connect"
  ],
  "engines": {
    "node": ">=4"
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/express-to-koa/issues"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "babelrc": true
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/preset-env": "^7.3.4",
    "ava": "^1.3.1",
    "axios": "^0.18.0",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0"
  }
}
